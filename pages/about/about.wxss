}

.h2 {
  clear: both;
  border-width: 1px 0;
  color: #666;
  text-align: center;
  font-size: 28rpx;
  line-height: 64rpx;
  font-weight: bold;
}

.clearfix {
    margin: 5px 15px;
    display: inline-block;
}
.goBack{
  margin-top: 50rpx;
