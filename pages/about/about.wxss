}
.img-like {
  width: 50rpx;
  height: 50rpx;
  display: inline-block;
  text-align: center;
  margin-right: 20rpx;
  margin-top: 8rpx;
}

.gravatarLikeImg {
  margin-top: 4px;
  border-radius: 50%;
